{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "no-title",
			"title": "No title header",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "post-with-sidebar",
			"title": "Post with sidebar",
			"postTypes": [
				"post"
			]
		},
		{
            "name": "archive-tainacan-collection",
            "title": "Tainacan collections list",
            "postTypes": [
                "tainacan-collection"
            ]
        }
	],
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"text": true,
			"palette": [
				{
					"color": "#A0303A",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#f2f2f2",
					"name": "Alternative background",
					"slug": "background-alt"
				},
				{
					"color": "#4f4f4f",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#262626",
					"name": "Alternative foreground",
					"slug": "foreground-alt"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				}
			],
			"duotone": [
				{
					"colors": [
						"#762b00",
						"#df834e"
					],
					"name": "Primary variations",
					"slug": "primary-variations"
				},
				{
					"colors": [
						"#262626",
						"#A0303A"
					],
					"slug": "foreground-alt-and-primary",
					"name": "Alternative Foreground and primary"
				},
				{
					"colors": [
						"#A0303A",
						"#ffffff"
					],
					"slug": "primary-and-background",
					"name": "Primary and background"
				},
				{
					"colors": [
						"#ffffff",
						"#A0303A"						
					],
					"slug": "background-and-primary",
					"name": "Background and primary"
				}
			],
			"gradients": []
		},
		"custom": {
			"spacing": {
				"small": "max(1.25rem, 5vw)",
				"large": "clamp(4rem, 10vw, 8rem)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"one-column": "calc(100vw/24)"
			}
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"%",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": true,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Headings (Montserrat)",
					"slug": "headings",
					"fontFace": [
						{
							"fontFamily": "\"Montserrat\"",
							"fontWeight": "300 400 600 800 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Montserrat-VariableFont_wght.ttf" ]
						},
						{
							"fontFamily": "\"Montserrat\"",
							"fontWeight": "300 400 600 800 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Montserrat-Italic-VariableFont_wght.ttf" ]
						}
					]
				},
				{
					"fontFamily": "\"Mulish\", sans-serif",
					"name": "Default (Mulish)",
					"slug": "default",
					"fontFace": [
						{
							"fontFamily": "\"Mulish\"",
							"fontWeight": "300 400 600 800 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Mulish-VariableFont_wght.ttf" ]
						},
						{
							"fontFamily": "\"Mulish\"",
							"fontWeight": "300 400 600 800 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Mulish-Italic-VariableFont_wght.ttf" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "0.9375rem",
					"fluid": {
						"min": "0.75rem",
						"max": "0.9375rem"
					},
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "1.125rem",
					"fluid": {
						"min": "1.0rem",
						"max": "1.125rem"
					},
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "1.25rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					},
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "1.5625rem",
					"fluid": {
						"min": "1.375rem",
						"max": "1.5625rem"
					},
					"slug": "medium"
				},
				{
					"name": "large",
					"size": "2.1875rem",
					"fluid": {
						"min": "1.875rem",
						"max": "2.1875rem"
					},
					"slug": "large"
				},
				{
					"name": "x-large",
					"size": "3.125rem",
					"fluid": {
						"min": "2.875rem",
						"max": "3.125rem"
					},
					"slug": "x-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		},
		"layout": {
			"contentSize": "1110px",
			"wideSize": "1400px"
		}
