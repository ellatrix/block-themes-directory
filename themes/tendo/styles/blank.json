{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Blank",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#899499",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#36454F",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#E5E4E2",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp-preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"elements": {
			"button": {
				"color": {
					"text": "var(--wp--preset--color--base)",
					"background": "var(--wp--preset--color--secondary)"
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				}
			}
		}
	}
}