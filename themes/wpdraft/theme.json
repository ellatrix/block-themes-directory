{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"color": {
			"custom": false,
			"background": false,
			"text":false,
			"customDuotone": false,
			"defaultPalette": false,
			"customGradient": false,
			"defaultGradients": false,
			"gradients": [],
			"duotone": [],
			"link": false
		},
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"spacing": {
			"margin": false,
			"padding": false,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"blockGap": null
		},
		"typography": {
			"lineHeight": false,
			"letterSpacing":false,
			"customFontSize": false,
			"fontWeight": false,
			"fontStyle": false,
			"textDecoration":false,
			"textTransform": false,
			"fontFamilies": [],
			"dropCap": false,
			"fontSizes": []
		},
		"layout": {
			"contentSize": "min( calc( 100% - var(--gutter, 2rem) * 2 ), var(--content-size, 60rem) )",
			"wideSize": "min( calc( 100% - var(--gutter, 2rem) * 2 ), var(--wide-size, 80rem) )"
		},
		"blocks": {
			"core/paragraph": {
				"border": {},
				"color": {},
				"custom": {},
				"layout": {},
				"spacing": {},
				"typography": {}
			},
			"core/button": {
				"border": {
					"radius": false
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/button": {
				"color": {
					"background": "transparent"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0",
						"left": "0",
						"right": "0"
					}
				}
			}
		}
	}
}