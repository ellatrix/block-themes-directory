{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "front",
			"title": "Front Page",
			"postTypes": [
				"page"
			]
		}
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"settings": {
		"layout": {
			"contentSize": "580px",
			"wideSize": "1180px"
		},
		"color": {
			"palette": [
				{
					 "slug": "primary",
					 "color": "#EEEBE6",
					 "name": "Primary"
				},
				{
					 "slug": "text-main",
					 "color": "#4F4F4F",
					 "name": "Text"
				},
				{
					"slug": "text-light",
					"color": "#767676",
					"name": "Text Light"
				},
				{
					 "slug": "text-dark",
					 "color": "#242424",
					 "name": "Text Dark"
				},
				{
					 "slug": "overlay",
					 "color": "#5B503B",
					 "name": "Overlay"
				}
			],
			"link": true
		},
