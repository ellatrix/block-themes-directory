{
	"version": 2,
	"customTemplates": [
		{
			"name": "page-no-footer-gap",
			"title": "Page no footer gap",
			"postTypes": [ "page" ]
		},
		{
			"name": "page-no-title-no-gap",
			"title": "Page no title no gap",
			"postTypes": [ "page" ]
		},
		{
			"name": "page-no-title",
			"title": "Page no title",
			"postTypes": [ "page" ]
		},
		{
			"name": "page-blank-canvas",
			"title": "Page no header no footer",
			"postTypes": [ "page" ]
		},
		{
			"name": "single-with-featured-image",
			"title": "Post with featured image",
			"postTypes": [ "post" ]
		}
	],
	"templateParts": [
		{
			"name": "header",
			"area": "header",
			"title": "Header"
		},
		{
			"name": "footer",
			"area": "footer",
			"title": "Footer"
		},
		{
			"name": "comments",
			"area": "uncategorized",
			"title": "Comments"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": true,
			"defaultGradients": true,
			"palette": [
				{
					"slug": "very-dark-gray",
					"name": "Very dark gray",
					"color": "#535354"
				},
				{
					"slug": "light-gray",
					"name": "Light gray",
					"color": "#dde3e9"
				},
				{
					"slug": "very-light-gray",
					"name": "Very light gray",
					"color": "#f2f2f2"
				},
				{
					"slug": "very-pale-blue",
					"name": "Very pale blue",
					"color": "#d7eaf7"
				},
				{
					"slug": "vivid-pink",
					"name": "Vivid pink",
					"color": "#ce0390"
				}
			],
			"gradients": [
				{
					"slug": "vivid-cyan-blue-to-vivid-purple-alt",
					"name": "Vivid cyan blue to vivid purple alternative",
					"gradient": "linear-gradient(23deg, var(--wp--preset--color--vivid-cyan-blue) 0%, var(--wp--preset--color--vivid-purple) 66%)"
				},
				{
					"slug": "pale-cyan-blue-to-vivid-purple",
					"name": "Pale cyan blue to vivid purple",
					"gradient": "linear-gradient(23deg, var(--wp--preset--color--pale-cyan-blue) 0%, var(--wp--preset--color--vivid-purple) 66%)"
				},
				{
					"slug": "vivid-cyan-blue-to-vivid-pink",
					"name": "Vivid cyan blue to vivid pink",
					"gradient": "linear-gradient(23deg, var(--wp--preset--color--pale-cyan-blue) 0%, var(--wp--preset--color--vivid-pink) 83%)"
				},
				{
					"slug": "pale-pink-to-vivid-red",
					"name": "Pale pink to vivid red",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--pale-pink) 0%, var(--wp--preset--color--vivid-red) 100%)"
				},
				{
					"slug": "pale-pink-to-vivid-purple",
					"name": "Pale pink to vivid purple",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--pale-pink) 0%, var(--wp--preset--color--vivid-purple) 100%)"
				},
				{
					"slug": "vivid-red-to-vivid-purple",
					"name": "Vivid red to vivid purple",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--vivid-red) 0%, var(--wp--preset--color--vivid-purple) 100%)"
				},
				{
					"slug": "vivid-red-to-vivid-pink",
					"name": "Vivid red to vivid pink",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--vivid-red) 0%, var(--wp--preset--color--vivid-pink) 90%)"
				},
				{
					"slug": "gray-circular-bands",
					"name": "Gray circular bands",
					"gradient": "radial-gradient(circle at top left, var(--wp--preset--color--light-gray) 0%, var(--wp--preset--color--light-gray) 22%, var(--wp--preset--color--very-light-gray) 22%, var(--wp--preset--color--very-light-gray) 32%, #fafafa 32%, #fafafa 42%, #ffffff 42%)"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Roboto,sans-serif",
					"name": "Roboto",
					"slug": "roboto",
					"google": "family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900"
				},
				{
					"fontFamily": "Poppins,sans-serif",
					"name": "Poppins",
					"slug": "poppins",
					"google": "family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900"
				},
				{
					"fontFamily": "Merriweather,serif",
					"name": "Merriweather",
					"slug": "merriweather",
					"google": "family=Merriweather:ital,wght@0,300;0,400;0,700;0,900;1,300;1,400;1,700;1,900"
				},
				{
					"fontFamily": "\"Ms Madi\",cursive",
					"name": "Ms Madi",
					"slug": "ms-madi",
					"google": "family=Ms+Madi"
				}
			],
			"fontSizes": [
				{ "slug": "small", "name": "Small", "size": "13px" },
				{ "slug": "normal", "name": "Normal", "size": "16px" },
				{ "slug": "medium", "name": "Medium", "size": "20px" },
				{ "slug": "large", "name": "Large", "size": "36px" },
				{ "slug": "x-large", "name": "XL", "size": "48px" },
				{ "slug": "xx-large", "name": "2XL", "size": "60px" }
			]
		},
		"layout": {
			"contentSize": "min( calc(100vw - 4rem), 48rem)",
			"wideSize": "min( calc(100vw - 4rem), 72rem)",
			"outerPadding": "0.5rem"
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--vivid-purple)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "26px"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.5",
			"fontWeight": "normal",
			"fontStyle": "normal"
		},
		"spacing": {
			"blockGap": "1rem"
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"textTransform": "uppercase",
					"lineHeight": "1"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--black)"
						}
					}
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontWeight": "500",
					"fontStyle": "normal",
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/post-title" : {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--black)"
						}
					}
				}
			},
			"core/post-excerpt": {
				"color": {
					"text": "var(--wp--preset--color--very-dark-gray)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--very-dark-gray)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"textTransform": "uppercase"
				}
			},
			"core/post-author": {
				"color": {
					"text": "var(--wp--preset--color--vivid-purple)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"textTransform": "uppercase"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/button": {
				"color": {
					"background": "var:preset|color|vivid-purple",
					"text": "var:preset|color|white"
				},
				"border": {
					"radius": "50px"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var:preset|color|vivid-purple"
				},
				"color": {
					"background": "var:preset|color|light-gray"
				}
			},
			"core/code": {
				"typography": {
					"fontSize": "var:preset|font-size|normal"
				},
				"color": {
					"background": "var:preset|color|black",
					"text": "var:preset|color|white"
				}
			}
		}
	}
}