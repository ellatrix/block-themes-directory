      "core/button": {
        "border": {
          "radius": "0px"
        },
        "color": {
          "background": "var(--wp--preset--color--ti-accent)",
          "text": "var(--wp--preset--color--ti-fg-alt)"
        },
        "spacing": {
          "padding": {
            "bottom": "16px",
            "left": "32px",
            "right": "32px",
            "top": "16px"
          }
        },
        "typography": {
