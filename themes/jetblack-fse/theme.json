		"custom": {
			"spacing": {
				"small": "max(1.25rem, 5vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)",
					"heading-first": "clamp(36px,3vw,3.2875rem)",
					"heading-second": "clamp(28px,2.6vw,2.475rem)",
					"heading-third": "clamp(20px,1.3vw,1.60rem)",
					"heading-fourth": "clamp(19px,1.2vw,1.5875rem)",
					"heading-five": "clamp(17px,1.1vw,1.3125rem)",
					"heading-sixth": "clamp(17px,1vw,1.125rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.3,
					"medium": 1.4,
					"normal": 1.75
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
