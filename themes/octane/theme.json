{
  "version": 1,
  "templateParts": [
    {
      "name": "header",
      "area": "header"
    },
    {
      "name": "footer",
      "area": "footer"
    }
  ],
  "customTemplates": [
    {
      "name": "blank-canvas",
      "title": "Blank Canvas - good for flash pages - without header footer - paint what you want"
    },
    {
      "name": "page-no-title",
      "title": "Page with no title"
    }
  ],
  "settings": {
    "border": {
      "customColor": true,
      "customRadius": true,
      "customStyle": true,
      "customWidth": true
    },
    "color": {
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "link": false,
      "palette": [
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Science Blue",
          "slug": "science-blue",
          "color": "#006DD0"
        },
        {
          "name": "Spring Wood",
          "slug": "spring-wood",
          "color": "#f5f0ed"
        },
        {
          "name": "Jumbo",
          "slug": "jumbo",
          "color": "#707071"
        },
        {
          "name": "Cinnabar",
          "slug": "cinnabar",
          "color": "#E66239"
        },
        {
          "name": "Mystic",
          "slug": "mystic",
          "color": "#EAEFF3"
        },
        {
          "name": "Dull Lavender",
          "slug": "dull-lavender",
          "color": "#96AEE6"
        },{
          "name": "Cerulean",
          "slug": "cerulean",
          "color": "#00a1dd"
        },
        {
          "name": "Fuchsia Blue",
          "slug": "fuchsia-blue",
          "color": "#8249C8"
        }
      ],
      "gradients": [
        {
          "name": "Light Purlple to Dark Purple",
          "slug": "light-purple-dark-purple",
          "gradient": "linear-gradient(145deg,#c86dd7 0,#8249c8 100%)"
        },
        {
          "name": "Light Blue to Dark Blue",
          "slug": "light-blue-dark-blue",
          "gradient": "linear-gradient(145deg,#00a1dd 0,#005ad6 100%)"
        }
      ]
    },
    "custom": {},
    "layout": {
      "contentSize": "1100px",
      "wideSize": "1340px"
    },
    "spacing": {
      "customMargin": true,
      "customPadding": true,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ]
    },
    "typography": {
      "customFontSize": true,
      "customLineHeight": false,
      "dropCap": true,
      "fontSizes": [
        {
          "slug": "tiny",
          "name": "Tiny",
          "size": "12px"
        },
        {
          "slug": "small",
          "name": "Small",
          "size": "15px"
        },
        {
          "slug": "normal",
          "name": "Normal",
          "size": "18px"
        },
        {
          "slug": "medium",
          "name": "Medium",
          "size": "24px"
        },
        {
          "slug": "large",
          "name": "Large",
          "size": "36px"
        },
        {
          "slug": "huge",
          "name": "Huge",
          "size": "48px"
        },
        {
          "slug": "gigantic",
          "name": "Gigantic",
          "size": "60px"
        },
        {
          "slug": "massive",
          "name": "Massive",
          "size": "72px"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "Quicksand, sans-serif",
          "name": "Quicksand",
          "slug": "quicksand"
        },
        {
          "fontFamily": "\"Work Sans\", sans-serif",
          "name": "Work Sans",
          "slug": "work-sans"
        },
        {
          "fontFamily": "Roboto, sans-serif",
          "name": "Roboto",
          "slug": "roboto"
        },
        {
          "fontFamily": "\"Roboto Slab\", serif",
          "name": "Roboto Slab",
          "slug": "roboto-slab"
        },
        {
          "fontFamily": "Merriweather, serif",
          "name": "Merriweather",
          "slug": "merriweather"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "#ffffff",
      "text": "var(--wp--preset--color--black)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--cinnabar)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--cinnabar)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "calc(var(--wp--preset--font-size--medium) * 1.375)"
        },
        "color": {
          "text": "var(--wp--preset--color--cinnabar)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "calc(var(--wp--preset--font-size--medium) * 1.25)"
        },
        "color": {
          "text": "var(--wp--preset--color--cinnabar)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "calc(var(--wp--preset--font-size--medium) * 1.125)"
        },
        "color": {
          "text": "var(--wp--preset--color--cinnabar)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        },
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "calc(var(--wp--preset--font-size--medium) * 0.875)"
        },
        "color": {
          "text": "var(--wp--preset--color--cinnabar)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--work-sans)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "1.6",
      "fontWeight": "300",
      "fontStyle": "normal"
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--quicksand)",
          "fontSize": "var(--wp--preset--font-size--large)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--cinnabar)"
            }
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--quicksand)",
          "fontWeight": "400",
          "fontStyle": "normal"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--quicksand)"
        }
      },
      "core/post-date": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/post-author": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/post-terms": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/button": {
        "border": {
          "radius": "6px"
        },
        "color": {
          "text": "var(--wp--preset--color--white)",
          "background": "var(--wp--preset--color--cinnabar)"
        }
      }
    }
  }
}