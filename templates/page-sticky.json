[{"title":"Blockpress","html":"<!-- wp:template-part {\"slug\":\"header\",\"tagName\":\"header\",\"className\":\"site-header\"} /-->\n<!-- wp:group {\"tagName\":\"main\",\"style\":{\"spacing\":{\"padding\":{\"bottom\":\"40px\"}}},\"className\":\"site-content is-style-no-margin\"} -->\n<main class=\"site-content is-style-no-margin wp-block-group\" style=\"padding-bottom:40px\">\n    <!-- wp:group {\"tagName\":\"main\",\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"bottom\":\"40px\"}}},\"className\":\"site-content is-style-no-margin\",\"layout\":{\"inherit\":true}} -->\n    <main class=\"wp-block-group alignfull site-content is-style-no-margin\" style=\"padding-bottom:40px\">\n        <!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"40px\",\"bottom\":\"40px\"},\"blockGap\":\"15px\"}},\"layout\":{\"inherit\":false}} -->\n        <div class=\"wp-block-group alignwide\" style=\"padding-top:40px;padding-bottom:40px\">\n            <!-- wp:post-terms {\"term\":\"category\",\"className\":\"is-style-blockpress-tags-primary\"} /-->\n\n            <!-- wp:post-title {\"level\":1,\"align\":\"wide\",\"fontSize\":\"max-huge\"} /-->\n\n            <!-- wp:post-date {\"style\":{\"typography\":{\"textTransform\":\"uppercase\"}},\"textColor\":\"lightgrey\",\"fontSize\":\"xsmall\"} /-->\n        </div>\n        <!-- /wp:group -->\n\n        <!-- wp:group {\"align\":\"full\",\"layout\":{\"inherit\":true}} -->\n        <div class=\"wp-block-group alignfull\">\n            <!-- wp:columns {\"align\":\"wide\",\"className\":\"fr-tablet-collapse\"} -->\n            <div class=\"wp-block-columns alignwide fr-tablet-collapse\">\n                <!-- wp:column {\"width\":\"120px\",\"frhideOnMobile\":true,\"frhideOnTablet\":true,\"frsetSticky\":true} -->\n                <div class=\"wp-block-column fr-mobile-hide fr-tablet-hide fr-position-sticky\" style=\"flex-basis:120px\">\n                    <!-- wp:social-links {\"layout\":{\"type\":\"flex\",\"orientation\":\"vertical\"}} -->\n                    <ul class=\"wp-block-social-links\">\n                        <!-- wp:social-link {\"url\":\"#\",\"service\":\"wordpress\"} /-->\n\n                        <!-- wp:social-link {\"url\":\"#\",\"service\":\"chain\"} /-->\n\n                        <!-- wp:social-link {\"url\":\"#\",\"service\":\"behance\"} /-->\n                    </ul>\n                    <!-- /wp:social-links -->\n                </div>\n                <!-- /wp:column -->\n\n                <!-- wp:column -->\n                <div class=\"wp-block-column\">\n                    <!-- wp:post-content {\"layout\":{\"inherit\":true}} /-->\n\n                    <!-- wp:post-comments /-->\n                </div>\n                <!-- /wp:column -->\n\n                <!-- wp:column {\"width\":\"5%\"} -->\n                <div class=\"wp-block-column\" style=\"flex-basis:5%\"></div>\n                <!-- /wp:column -->\n\n                <!-- wp:column {\"width\":\"300px\",\"frsetSticky\":true} -->\n                <div class=\"wp-block-column fr-position-sticky\" style=\"flex-basis:300px\">\n                    <!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"top\":\"100px\",\"bottom\":\"100px\",\"right\":\"20px\",\"left\":\"20px\"}},\"color\":{\"background\":\"#f5f8ff\"}},\"textColor\":\"lightgrey\"} -->\n                    <div class=\"wp-block-group has-lightgrey-color has-text-color has-background\"\n                        style=\"background-color:#f5f8ff;padding-top:100px;padding-right:20px;padding-bottom:100px;padding-left:20px\">\n                        <!-- wp:paragraph {\"align\":\"center\"} -->\n                        <p class=\"has-text-align-center\">It can be banner here</p>\n                        <!-- /wp:paragraph -->\n                    </div>\n                    <!-- /wp:group -->\n                </div>\n                <!-- /wp:column -->\n            </div>\n            <!-- /wp:columns -->\n        </div>\n        <!-- /wp:group -->\n    </main>\n    <!-- /wp:group -->\n</main>\n<!-- /wp:group -->\n<!-- wp:template-part {\"slug\":\"footer\",\"tagName\":\"footer\",\"className\":\"site-footer is-style-no-margin\"} /-->"}]